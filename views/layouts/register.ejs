<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet"
            integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
        <link rel="stylesheet" href="/css/login.css">

        <body>
            <div class="app">
                <form action="/admin/register" method="post">
                    <div>
                        <!-- {{#if error}}
                        <p class="err-msg">Username or password is incorrect</p>
                        {{/if }} -->
                        <h1>SIGNIN</h1>
                    </div>
                    <div class="form-group">
                        <label>Username:</label>
                        <input type="text" name="username" id="username" placeholder="Enter your username" required />
                    </div>
                    <div class="form-group">
                        <label>Password:</label>
                        <input type="password" name="password" id="password" placeholder="Enter your password"
                            required />
                    </div>
                    <p><a href="/" class="backhome p-lg-3">&lt - back to วิทยาการคอมพิวเตอร์&nbsp;|&nbsp;มก.</a></p>
                    <div>
                        <input type="submit" value="Signin" />
                    </div>
                    <div class="text-center mt-4 text-light">
                        <p>Already member? <a class="home" href="/admin">Login</a></p>
                    </div>
                </form>
            </div>
        </body>

        </html>